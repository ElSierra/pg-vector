{
  "build": {
    "env": {
      "POSTGRES_USER": "postgres",
      "POSTGRES_PASSWORD": "postgres",
      "POSTGRES_DB": "pgvector_db"
    }
  },
  "deploy": {
    "healthcheckPath": "/health",
    "healthcheckTimeout": 60,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
